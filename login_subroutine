
def securelogin() : 
  for i in range(5) :
    print("SECURE LOGIN")
    username = input("Username > ")
    password = input ( "Password >")
  
    if username == "mark" and password =="mark":
        print('Welcome Mark!')
        break
    elif username == "roxanne" and password =="roxanne":
        print ("Welcome Roxanne!")
        break
    else:
        continue

securelogin()






